{
  "name": "smart-contact/trustpilot-api-service",
  "description": "Service for trustpilot API",
  "keywords": [
    "smart-contact",
    "trustpilot-api-service"
  ],
  "homepage": "https://github.com/smart-contact/trustpilot-service",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Mameli Federico",
      "email": "federico.mameli@smart-contact.it",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "illuminate/support": "^8.0"
  },
  "require-dev": {
    "orchestra/testbench": "^6.0",
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "SmartContact\\TrustpilotService\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SmartContact\\TrustpilotService\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "SmartContact\\TrustpilotService\\TrustpilotServiceServiceProvider"
      ],
      "aliases": {
        "TrustpilotService": "SmartContact\\TrustpilotService\\TrustpilotServiceFacade"
      }
    }
  }
}
